<head>
    <meta charset="utf-8">
    <meta name="description" content="Macau Slot">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>S-B</title>
    <% 
        assets.chunks.map(function(chunk){
            if(chunk.files[1]){
    %>
        <link href="./build/<%= chunk.files[1] %>" rel="stylesheet">
    <% }})%>
</head>
<body>

<div id="app"><%- html %></div>
<script>
    window.__INITIAL_STATE__ = <%- JSON.stringify(initialState) %>;
</script>  
<script src="/socket.io/socket.io.js"></script>
<script>console.info(<%- JSON.stringify(assets) %>)</script>
    <% 
        assets.chunks.map(function(chunk){
            if(chunk.files[0] && chunk.names[0] === 'vendors'){
    %>
        <script src="./build/<%= chunk.files[0] %>"></script>
    <% }})%>

    <% 
        assets.chunks.map(function(chunk){
            if(chunk.files[0] && chunk.names[0] === 'app'){
    %>
        <script src="./build/<%= chunk.files[0] %>"></script>
    <% }})%>

    <% 
        assets.chunks.map(function(chunk){
            if(chunk.files[0] && chunk.names[0] !== 'app' && chunk.names[0] !== 'vendors'){
    %>
        <script src="./build/<%= chunk.files[0] %>"></script>
    <% }})%>
</body>
</html>